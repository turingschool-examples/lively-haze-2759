<h1><%= @dish.name %></h1>
<p><%= @dish.description %></p>

<h2>Ingredients:</h2>
<ul>
  <% @ingredients.each do |ingredient| %>
    <li><%= ingredient.name %></li>
  <% end %>
</ul>

<p>Total Calories: <%= @dish.total_calories %></p>

<p>Chef's Name: <%= @chef.name %></p>

<p><%= form_with model: @dish do |form| %>
  <%= form.label :id %>
  <%= form.text_field :id %>
  <%= form.submit %>
<% end %></p>
